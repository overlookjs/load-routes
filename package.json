{
  "name": "@overlook/load-routes",
  "version": "0.0.3",
  "description": "Load Overlook routes from files",
  "main": "index.js",
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookjs/load-routes.git"
  },
  "bugs": {
    "url": "https://github.com/overlookjs/load-routes/issues"
  },
  "dependencies": {
    "@overlook/route": "^0.1.0",
    "core-util-is": "^1.0.2",
    "recursive-readdir": "^2.2.2"
  },
  "devDependencies": {
    "@overlookmotel/eslint-config": "^3.0.2",
    "@overlookmotel/eslint-config-jest": "^1.1.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^23.6.0",
    "jest-extended": "^0.11.1"
  },
  "keywords": [
    "overlook",
    "framework",
    "load",
    "loader",
    "route",
    "routes",
    "files",
    "directory",
    "tree"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint '*.js' '.*.js' '**/*.js' '**/.*.js'",
    "lint-fix": "eslint '*.js' '.*.js' '**/*.js' '**/.*.js' --fix",
    "test-main": "cross-env NODE_ENV=production jest",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "cross-env COVERAGE=true NODE_ENV=production jest --coverage",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm test; fi"
  },
  "engines": {
    "node": ">=8"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
